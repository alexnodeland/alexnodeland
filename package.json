{
  "name": "alexnodeland",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build:worker": "npx webpack --config webpack.worker.config.js",
    "build": "npm run build:worker && gatsby build",
    "develop": "npm run build:worker && gatsby develop",
    "start": "npm run build:worker && gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=__tests__/unit",
    "test:integration": "jest --testPathPattern=__tests__/integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:coverage:full": "node scripts/test-coverage.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx,scss,md}",
    "code-quality": "npm run type-check && npm run lint && npm run format",
    "code-quality:fix": "npm run type-check && npm run lint:fix && npm run format",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexnodeland/alexnodeland.git"
  },
  "keywords": [
    "portfolio",
    "gatsby",
    "react",
    "typescript",
    "scss",
    "ai-engineering",
    "devops",
    "resume"
  ],
  "author": "Alex Nodeland <alex@ournature.studio>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexnodeland/alexnodeland/issues"
  },
  "homepage": "https://github.com/alexnodeland/alexnodeland#readme",
  "description": "Portfolio website for Alex Nodeland - Senior AI Engineer & Technical Consultant specializing in AI system architecture, DevOps automation, and production-ready AI infrastructure.",
  "dependencies": {
    "@huggingface/transformers": "^3.7.3",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/three": "^0.180.0",
    "docx": "^9.5.1",
    "file-saver": "^2.0.5",
    "gatsby": "^5.15.0",
    "gatsby-plugin-image": "^3.15.0",
    "gatsby-plugin-manifest": "^5.15.0",
    "gatsby-plugin-react-helmet": "^6.15.0",
    "gatsby-plugin-sass": "^6.15.0",
    "gatsby-plugin-sharp": "^5.15.0",
    "gatsby-plugin-typescript": "^5.15.0",
    "gatsby-source-filesystem": "^5.15.0",
    "gatsby-transformer-remark": "^6.15.0",
    "gatsby-transformer-sharp": "^5.15.0",
    "highlight.js": "^11.11.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "three": "^0.180.0",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/register": "^7.28.3",
    "@playwright/test": "^1.40.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/file-saver": "^2.0.7",
    "@types/jest": "^30.0.0",
    "@types/react-helmet": "^6.1.11",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "babel-jest": "^29.7.0",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^16.2.0",
    "msw": "^2.0.0",
    "playwright": "^1.40.0",
    "prettier": "^3.6.2",
    "sass": "^1.93.1",
    "sass-true": "^9.0.0",
    "ts-node": "^10.9.1",
    "webpack-cli": "^6.0.1"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{scss,css,md,json}": [
      "prettier --write"
    ]
  }
}
